# Calculate pkg(xfce-base/exo)!=||pkg(www-client/qupzilla)!=||pkg(www-client/chromium)!=||pkg(www-client/firefox)!=
<?xml version="1.0" encoding="UTF-8"?>
<config>
#?os_locale_language!=ru#
  <group name="Browser">
#!os_locale_language#
  <group name="браузере">
#os_locale_language#
#?pkg(xfce-base/exo)!=#
    <option name="command" value="exo-open %1 &amp;"/>
#pkg#
#?pkg(www-client/qupzilla)!=&&pkg(xfce-base/exo)==&&pkg(www-client/chromium)==&&pkg(www-client/firefox)==#
    <option name="command" value="qupzilla %1 &amp;"/>
#pkg#
#?pkg(www-client/chromium)!=&&pkg(xfce-base/exo)==&&pkg(www-client/qupzilla)==&&pkg(www-client/firefox)==#
    <option name="command" value="chromium %1 &amp;"/>
#pkg#
#?pkg(www-client/firefox)!=&&pkg(xfce-base/exo)==&&pkg(www-client/qupzilla)==&&pkg(www-client/chromium)==#
    <option name="command" value="firefox %1 &amp;"/>
#pkg#
  </group>
</config>
